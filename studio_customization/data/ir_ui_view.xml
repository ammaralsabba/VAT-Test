                  <field name="quantity" class="oe_inline"/>
                  <field name="uom_id" class="oe_inline" groups="uom.group_uom"/>
                </div>
                <field name="price_unit"/>
                <field name="x_studio_input_tax_credit" string="Input Tax Credit" widget="label_selection"/>
